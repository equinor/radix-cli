{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Lauch radix-cli list applications",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {},
      "args": ["list", "applications"]
    },
    {
      "name": "Lauch radix-cli build deploy application",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "APP_SERVICE_ACCOUNT_TOKEN": "token"
      },
      "args": [
        "--token-environment",
        "--context",
        "playground",
        "trigger",
        "build-deploy",
        "--application",
        "echo",
        "-b",
        "master",
        "-f"
      ]
    },
    {
      "name": "Lauch radix-cli deploy application",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "APP_SERVICE_ACCOUNT_TOKEN": "token"
      },
      "args": [
        "--token-environment",
        "--context",
        "development",
        "trigger",
        "deploy",
        "--application",
        "xample-deploy-only",
        "-e",
        "prod",
        "-f"
      ]
    },
    {
      "name": "Lauch radix-cli set environment secret",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "APP_SERVICE_ACCOUNT_TOKEN": "token"
      },
      "args": [
        "--token-environment",
        "--from-config",
        "--await-reconcile",
        "set",
        "environment-secret",
        "--context",
        "playground",
        "-e",
        "qa",
        "--component",
        "api",
        "-s",
        "APPINSIGHTS_INSTRUMENTATIONKEY",
        "-v",
        "iknu-test"
      ]
    },
    {
      "name": "Lauch radix-cli follow component",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "APP_SERVICE_ACCOUNT_TOKEN": "token"
      },
      "args": [
        "follow",
        "component",
        "--application",
        "xample-2-chat",
        "--context",
        "playground",
        "-e",
        "dev",
        "--component",
        "socket-server"
      ]
    },
    {
      "name": "Lauch radix-cli follow environment",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "APP_SERVICE_ACCOUNT_TOKEN": "token"
      },
      "args": [
        "follow",
        "environment",
        "--application",
        "xample-2-chat",
        "--context",
        "playground",
        "-e",
        "dev"
      ]
    },
    {
      "name": "Lauch radix-cli follow job",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "APP_SERVICE_ACCOUNT_TOKEN": "token"
      },
      "args": [
        "--token-environment",
        "--context",
        "playground",
        "follow",
        "job",
        "--application",
        "echo",
        "-j",
        "radix-pipeline-20191227190153-aysdr"
      ]
    },
    {
      "name": "Lauch radix-cli get environment from branch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "APP_SERVICE_ACCOUNT_TOKEN": "token"
      },
      "args": [
        "--token-environment",
        "--from-config",
        "get-config",
        "branch-environment",
        "-b",
        "master"
      ]
    },
    {
      "name": "Lauch radix-cli create application",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "APP_SERVICE_ACCOUNT_TOKEN": "token"
      },
      "args": [
        "--token-environment",
        "create",
        "application",
        "--application",
        "radix-example-arm-template",
        "--cluster",
        "iknu-test-machine-user",
        "--repository",
        "https://github.com/equinor/radix-example-arm-template",
        "--owner",
        "some@hotmail.com",
        "--shared-secret",
        "AnySharedSecret"
      ]
    },
    {
      "name": "Lauch radix-cli get application",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "APP_SERVICE_ACCOUNT_TOKEN": "token"
      },
      "args": [
        "--token-environment",
        "get",
        "application",
        "--application",
        "xample-2-chat",
        "--cluster",
        "iknu-test-machine-user"
      ]
    }
  ]
}
